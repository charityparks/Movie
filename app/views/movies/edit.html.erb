<%= form_for(@movie) do |f| %>
<%= f.label :title %><br>
<%= f.text_field :title %><br>

<%= f.label :actor %><br>
<%= f.text_field :actor %><br>
 
<%= f.label :role %><br>
<%= f.text_field :role %><br>

<%= f.label :rating %><br>
<%= f.text_field :rating %><br>

<%= f.label :genre %><br>
<%= f.text_field :genre %><br>

<%= f.label :description %><br>
<%= f.text_area :description %><br>
 
<%= f.submit "Submit Changes" %>
<% end %>
